<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>PoolConexiones</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>es.studium.LoginServlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HazAlgoServlet</servlet-name>
		<servlet-class>es.studium.LoginServlet.HazAlgoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>es.studium.LoginServlet.LogoutServlet</servlet-class>
	</servlet>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>